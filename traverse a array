#include<iostream>
using namespace std;
int main()
{
    int i,n,sum=0;
    
    cin>>n;
    int arr[n];
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
        sum =+ sum+1;//for calculating the number of elements in the array
    }
    cout<<sum;//print the number of element
    cout<<endl;
    for(i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }

}
