#include<iostream>
using namespace std;
class node
{
    public:
    int data;
    node * next;
    node(int data)
    {
        this->data=data;
        next=NULL;
    }
};
int main()
{
    node * n3 = new node(1);
    node * n4 = new node(2);
    node * n5 = new node(3);
    node * head = n3;
    n3->next=n4;
    n4->next=n5;
    cout<<n3->data<<endl;
    cout<<n3->next<<endl;//return the address of n4
    cout<<n4<<endl;// return the address of n4

}
